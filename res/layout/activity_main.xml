<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <Button
        android:id="@+id/btnImsi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:contentDescription="汉字二"
        android:text="@string/RbtnImsi" />

    <TextView
        android:id="@+id/txtImsi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/btnImsi"
        android:layout_marginTop="16dp"
        android:contentDescription="txtimsicontentd"
        android:text="@string/RtxtImsi"
        tools:context=".MainActivity" />

</RelativeLayout>
